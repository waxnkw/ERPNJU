<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane prefHeight="500.0" prefWidth="400.0" styleClass="background" stylesheets="@LoginUi.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.loginUi.RegisterUiController">
   <center>
      <GridPane alignment="CENTER" hgap="20.0" prefHeight="319.0" prefWidth="502.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="288.0" minWidth="10.0" prefWidth="192.0" />
          <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="188.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="注册账号：">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <Text fx:id="oldPwPromptText" strokeType="OUTSIDE" strokeWidth="0.0" text="默认密码：" GridPane.rowIndex="1">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <TextField fx:id="userIdTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="164.0" promptText="请输入您的ID" GridPane.columnIndex="1">
               <font>
                  <Font size="18.0" />
               </font></TextField>
            <PasswordField fx:id="oldPwTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="165.0" promptText="请输入默认密码" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font size="18.0" />
               </font></PasswordField>
            <PasswordField fx:id="newPwTextField" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="167.0" promptText="请输入新密码" GridPane.columnIndex="1" GridPane.rowIndex="2">
               <font>
                  <Font size="18.0" />
               </font></PasswordField>
            <Text fx:id="newPwPromptText" strokeType="OUTSIDE" strokeWidth="0.0" text="新密码：" GridPane.rowIndex="2">
               <font>
                  <Font size="24.0" />
               </font>
            </Text>
            <Text fx:id="promptText" strokeType="OUTSIDE" strokeWidth="0.0" text="欢迎新用户！" GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.rowIndex="3">
               <font>
                  <Font size="25.0" />
               </font>
            </Text>
            <HBox alignment="TOP_CENTER" prefHeight="61.0" prefWidth="600.0" spacing="50.0" GridPane.columnSpan="2" GridPane.rowIndex="4">
               <children>
                  <Button fx:id="confirmButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#confirm" prefHeight="35.0" prefWidth="75.0" styleClass="buttons" text="确认">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
                  <Button fx:id="cancleButton" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#cancle" prefHeight="35.0" prefWidth="75.0" styleClass="buttons" text="取消">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
         <BorderPane.margin>
            <Insets bottom="10.0" top="150.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
</BorderPane>
